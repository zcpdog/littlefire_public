<%if @grade.gradable_type == "Deal"%>
	var deal_field = $("#deal_<%=@grade.gradable_id%>");
	<%if @grade.agree%>
		var grade_field = deal_field.find(".deal-grade-up")
	<%else%>
		var grade_field = deal_field.find(".deal-grade-down")
	<%end%>
	var child = grade_field.children();
	grade_field.text(parseInt(grade_field.text())+1);
	grade_field.prepend(child);
	grade_field.attr("href","javascript:void(0)");
<%elsif @grade.gradable_type == "Comment"%>
	
<%end%>